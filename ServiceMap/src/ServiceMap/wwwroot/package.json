    "@angular/platform-browser-dynamic": "4.3.5",
    "@angular/platform-server": "4.3.5",
    "@angular/router": "4.3.5",
    "angular2-busy": "2.0.4",
    "core-js": "2.4.1",
    "ng2-bootstrap": "1.6.3",
    "ng2-toastr": "4.1.2",
    "primeng": "2.0.3",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.4.2",
    "systemjs": "0.20.2",
    "ts-helpers": "1.1.1",
